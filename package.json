{
  "name": "cypress-training",
  "version": "1.0.0",
  "description": "This is a Workshop about Cypress",
  "main": "index.js",
  "scripts": {
    "clean:reports": "rm -rf cypress/reports && mkdir cypress/reports",
    "clean:reportsWindows": "rmdir /S /Q cypress\\reports && mkdir cypress\\reports",
    "test": "cypress run --headless --browser chrome",
    "testWindows": "cypress run --headless --browser chrome",
    "test:open": "npm run lint && cypress open",
    "lint": "eslint ./cypress/e2e/**/*.ts",
    "lint:fix": "npm run lint -- --fix",
    "combine:reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "combine:reportsWindows": "mochawesome-merge cypress\\reports\\mocha\\*.json > cypress\\reports\\report.json",
    "generate:reports": "marge cypress/reports/report.json -f report -o cypress/reports",
    "generate:reportsWindows": "marge cypress\\reports\\report.json -f report -o cypress\\reports",
    "pretest": "npm run clean:reports",
    "pretestWindows": "npm run clean:reportsWindows",
    "posttest": "npm run combine:reports && npm run generate:reports",
    "posttestWindows": "npm run combine:reportsWindows && npm run generate:reportsWindows"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexca1703/cypress-training-newapproach.git"
  },
  "keywords": [
    "ui-testing",
    "dojo",
    "practice",
    "cypress"
  ],
  "author": "Alexander Martinez <alexca1703@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexca1703/cypress-training-newapproach/issues"
  },
  "homepage": "https://github.com/alexca1703/cypress-training-newapproach#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "cypress": "^12.7.0",
    "cypress-mochawesome-reporter": "^3.4.0",
    "cypress-multi-reporters": "^1.6.3",
    "eslint": "^8.36.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "typescript": "^5.0.2"
  }
}
